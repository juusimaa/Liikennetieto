{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"type": "object",
	"properties": {
		"parkingstation" :
		{
			"type": "array",
			"items":
			{
				"type": "object",
				"properties": {
					"id" :
					{
						"description": "ID for this parkingstation",
						"type": "integer"
					},
					"geom" :
					{
						"description": "Geometry for this parkingstation as GeoJSON, wrapped in a string",
						"type": "string"  
					},
					"name" :
					{
						"description": "Name for this parkingstation",
						"type": "string" 
					}					
				},
				"required": ["id", "geom", "name"]
			} 
		}    
	} 
}

